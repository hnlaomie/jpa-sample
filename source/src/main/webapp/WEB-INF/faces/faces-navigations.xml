<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!-- Top -->
	<navigation-rule>
		<from-view-id>/xhtml/*</from-view-id>
		<navigation-case>
			<from-outcome>top</from-outcome>
			<to-view-id>/xhtml/top.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<!-- Step1 -->
	<navigation-rule>
		<from-view-id>/xhtml/*</from-view-id>
		<navigation-case>
			<from-outcome>step1</from-outcome>
			<to-view-id>/xhtml/step1.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
